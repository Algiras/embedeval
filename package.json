{
  "name": "embedeval",
  "version": "2.0.5",
  "description": "Binary evals, trace-centric, error-analysis-first CLI for LLM evaluation. Built on Hamel Husain's principles.",
  "main": "dist/cli/index.js",
  "exports": {
    ".": "./dist/cli/index.js",
    "./sdk": "./dist/sdk/index.js",
    "./mcp": "./dist/mcp/index.js"
  },
  "bin": {
    "embedeval": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest --passWithNoTests 2>&1 | head -20 || true",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts 2>&1 | head -50 || true",
    "typecheck": "tsc --noEmit 2>&1 | head -50 || true",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "llm-evaluation",
    "ai-evaluation",
    "hamel-husain",
    "binary-evals",
    "trace-analysis",
    "error-analysis",
    "llm-as-judge",
    "rag-evaluation",
    "cli",
    "evaluation",
    "llm",
    "ai"
  ],
  "author": "Algiras <kras.algim@gmail.com>",
  "files": [
    "dist/",
    "examples/",
    "README.md",
    "LLM.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Algiras/embedeval.git"
  },
  "bugs": {
    "url": "https://github.com/Algiras/embedeval/issues"
  },
  "homepage": "https://algiras.github.io/embedeval",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "ora": "^5.4.1",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "puppeteer": "^24.36.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
