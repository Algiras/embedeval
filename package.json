{
  "name": "embedeval",
  "version": "1.0.0",
  "description": "CLI-based embedding evaluation system with A/B testing, human evaluation, and composable pipelines",
  "main": "dist/index.js",
  "bin": {
    "embedeval": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:local": "./scripts/test-local.sh",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .embedeval",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "embeddings",
    "evaluation",
    "rag",
    "retrieval",
    "cli",
    "ab-testing",
    "semantic-search",
    "vector-search",
    "llm",
    "ai",
    "machine-learning"
  ],
  "author": "Algiras <kras.algim@gmail.com>",
  "files": [
    "dist/",
    "examples/",
    "docker/",
    "scripts/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Algiras/embedeval.git"
  },
  "bugs": {
    "url": "https://github.com/Algiras/embedeval/issues"
  },
  "homepage": "https://github.com/Algiras/embedeval#readme",
  "dependencies": {
    "bullmq": "^5.0.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ioredis": "^5.3.2",
    "yaml": "^2.3.4",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "cli-progress": "^3.12.0",
    "openai": "^4.24.1",
    "@google/generative-ai": "^0.1.3",
    "handlebars": "^4.7.8",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
